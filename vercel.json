{
    "version": 2,
    "outputDirectory": "public",
    "framework": null,
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.6.1"
      }
    },
    "headers": [
      {
        "source": "/apis/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://cardify-frontend-git-dev-gaby02000s-projects.vercel.app" },
          { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
        ]
      }
    ],
    "routes": [
      {
        "src": "/(css|js|vendor|assets|build)/(.*)",
        "dest": "public/$1/$2"
      },
      {
        "src": "/(.*)",
        "dest": "api/index.php"
      }
    ]
  }